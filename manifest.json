{
    "name": "Dogears",
    "version": "0.133",
    "icons": {
        "16": "bkmk_small.png",
        "48": "bkmk.png",
        "128": "dogear.png"
    },
    "description": "Easily keep track of your place in a number of different sites (like HTML books for instance.)",
    "browser_action": {
        "default_icon": "bkmk_small.png",
        "popup": "popup.html"
    },
    "permissions": [
        "tabs", "http://*/*", "https://*/*"
    ],
    "content_scripts": [
    {
      "matches": ["http://*/*", "https://*/*"],
      "js": ["scroll.js"]
    }
    ]
}